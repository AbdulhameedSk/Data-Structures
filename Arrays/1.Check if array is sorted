
class Solution {
    boolean arraySortedOrNot(int[] arr, int n) {
        // code here
        boolean c=true;
        for(int i=0;i<n-1;i++){
            if(arr[i]>arr[i+1]){
                c=false;
                break;
            }
           
        }
        
        return c;
        
        
    }
}
